@Article{Ferrera2017Dynamic,
  author    = {Marcello Ferrera and Nathaniel Kinsey and Amr Shaltout and Clayton DeVault and Vladimir Shalaev and Alexandra Boltasseva},
  title     = {Dynamic nanophotonics \[Invited\]},
  journal   = {J. Opt. Soc. Am. B},
  year      = {2017},
  volume    = {34},
  number    = {1},
  pages     = {95--103},
  doi       = {10.1364/JOSAB.34.000095},
  url       = {http://josab.osa.org/abstract.cfm?URI=josab-34-1-95},
  month     = jan,
  abstract  = {The field of integrated plasmonics is multifaceted in a way that few other disciplines in applied science are, mainly due to its intrinsic \&\#x201C;hybrid\&\#x201D; nature of combining materials and strategies borrowed from electronics and photonics. Because of the multitude of angles under which the plasmonic world could be analyzed, and also because of the intrinsic interest behind this branch of physics, numerous review papers have been recently published with the attempt to exhaustively describe this subject and its possible future developments. However, despite the considerable literature already available, a few important aspects deserve a deeper investigation. Among these dark spots we find the lack of a general overview of active plasmonics, specifically focused on the possibility to dynamically alter the optical properties of the constituent plasmonic materials in order to gain full active control over the overall desired functionality. The present review focuses on the possibility to tune the optical properties of said components, deliberately neglecting those strategies relying on the dynamic properties of the dielectric constituent. The present work also will attempt to outline experimental and multidisciplinary aspects of tunable plasmonic devices, giving only a marginal overview of telecom applications, for which considerable literature is already available.},
  file      = {Ferrera2017Dynamic.pdf:Ferrera2017Dynamic.pdf:PDF},
  groups    = {NanophotonicsInGeneral},
  keywords  = {Integrated optics devices; Ultrafast devices; Artificially engineered materials ; Active or adaptive optics},
  owner     = {qxd},
  publisher = {OSA},
  timestamp = {2018-01-15},
}

