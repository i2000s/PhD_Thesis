@Article{Lee2015a,
  author    = {Siu-Chun Lee},
  title     = {Evanescent wave scattering at off-axis incidence on multiple cylinders located near a surface},
  journal   = {Journal of Quantitative Spectroscopy and Radiative Transfer},
  year      = {2015},
