@Article{Caceres1996,
  author    = {Manuel O. Cáceres and Ana K. Chattah},
  title     = {On the Schrödinger-Langevin picture and the master equation},
  journal   = {Physica A: Statistical Mechanics and its Applications},
  year      = {1996},
  volume    = {234},
