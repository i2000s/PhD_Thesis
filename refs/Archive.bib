@Article{Bellomo2014,
  author    = {G. Bellomo and A.P. Majtey and A.R. Plastino and A. Plastino},
  title     = {Quantum correlations from classically correlated states},
  journal   = {Physica A: Statistical Mechanics and its Applications},
  year      = {2014},
