@Article{Kippenberg2008,
  author    = {Kippenberg, T. J. and Vahala, K. J.},
  title     = {Cavity Optomechanics: Back-Action at the Mesoscale},
  journal   = {Science},
  year      = {2008},
  volume    = {321},
  number    = {5893},
  pages     = {1172--1176},
  doi       = {10.1126/science.1156032},
  eprint    = {http://www.sciencemag.org/cgi/reprint/321/5893/1172.pdf},
