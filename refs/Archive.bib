@Article{Salem2010,
  author    = {R Salem and Y Japha and J Chabé and B Hadad and M Keil and K A Milton and R Folman},
  title     = {Nanowire atomchip traps for sub-micron atom–surface distances},
  journal   = {New Journal of Physics},
  year      = {2010},
  volume    = {12},
  number    = {2},
  pages     = {023039},
  url       = {http://stacks.iop.org/1367-2630/12/i=2/a=023039},
  abstract  = {We present an analysis of magnetic traps for ultracold atoms based on current-carrying wires with sub-micron dimensions. We analyze the physical limitations of these conducting wires as well as how such miniaturized magnetic traps are affected by the nearby surface due to tunneling to the surface, surface thermal noise, electron scattering within the wire and the Casimir–Polder force. We show that wires with cross sections as small as a few tens of nanometers should enable robust operating conditions for coherent atom optics (e.g. tunneling barriers for interferometry). In particular, trap sizes of the order of the de Broglie wavelength become accessible, based solely on static magnetic fields, thereby bringing the atomchip a step closer to fulfilling its promise of a compact device for complex and accurate quantum optics with ultracold atoms.},
  file      = {Salem2010.pdf:Salem2010.pdf:PDF},
  groups    = {NeutralAtoms, TrappingAndRepulsing},
  owner     = {qxd},
  timestamp = {2014.12.09},
}

